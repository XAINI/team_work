.working-hours-approval-history
  .top-area
  .working-hours-approval-history-title
    %label
      %i.glyphicon.glyphicon-tags
      工时审核历史记录
  .working-hours-approval-history-box
    .container
      .row
        .span8
          %ul.nav.nav-tabs
            %li
              %a{href:"javascript:;"}="按日期分组"
            %li
              %a{href:"javascript:;"}="按项目分组"
            %li
              %a{href:"/test/working_hours_approval"}="按员工分组"
            %li.active
              %a{href:"javascript:;"}="工时审核历史记录"
    .select
      .first
        时间类型
        %select
          %option= "不指定"
          %option= "休假工时"
          %option= "加班工时"
          %option= "正常工时"
        申请人
        %select
          %option= ""
          %option= "员工1"
          %option= "员工2"
          %option= "员工3"
        所属项目
        %input
      .second
        工时开始时间
        %input{size:"16",type:"text",value:"12-02-2012"}
        结束时间
        %input.calendar2
        时期范围
        %select
          %option="不指定"
          %option="截至当前"
          %option="今年"
          %option="本季度"
          %option="本月"
          %option="本周"
      .third
        审批开始时间
        %input.calendar3
        结束时间
        %input.calendar4
        时期范围
        %select
          %option="不指定"
          %option="截至当前"
          %option="今年"
          %option="本季度"
          %option="本月"
          %option="本周"
        %button.btn.btn-primary="查询"
  .working-hours-approval-history-body
    %table.table.table-hover
      %thead
        %tr{style: "background-color: #EAEAEA;"}
          %th="操作"
          %th="日期"
          %th="填报人"
          %th="提交时间"
          %th="审批状态"
          %th="审批时间"
          %th="工时（小时）"
          %th="时间类型"
          %th="备注"
          %th="所属项目"
          %th="所属任务"
